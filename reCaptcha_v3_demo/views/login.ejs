<form class="form-signin">
    <img class="mb-4" src="/images/flower.svg" alt="" width="72" height="72">
    <h1 class="h3 mb-3 font-weight-normal">reCAPTCHA v3 Demo</h1>
    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" id="inputEmail" name="inputEmail" class="form-control" placeholder="Email address" required autofocus>
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" name="inputPassword" class="form-control" placeholder="Password" required>
    <button class="btn btn-lg btn-dark btn-block" type="submit">Sign in</button>
</form>

<!-- reCaptcha JS -->
<script src="https://www.google.com/recaptcha/api.js?render=6LeTEMMZAAAAAMlkG8F4v7OAjq1W6tkaLOmZuAdl"></script>

<!-- Custom JS -->
<script src="/client.js"></script>
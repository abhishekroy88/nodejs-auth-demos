<form class="form-signin" action="?" method="POST">
    <img class="mb-4" src="/images/flower.svg" alt="" width="72" height="72">
    <h1 class="h3 mb-3 font-weight-normal">reCaptcha v2 Demo</h1>
    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" id="inputEmail" class="form-control" placeholder="Email address" required autofocus>
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" class="form-control" placeholder="Password" required>
    <div class="form-group">
        <div class="g-recaptcha" data-sitekey="6LfLdMMZAAAAAIcIC1P5RsyUG1Vb27S0PbDNL687" data-callback="enableSubmit" data-expired-callback="recaptchaExpired"></div>
    </div>
    <button class="btn btn-lg btn-primary btn-block" type="submit" disabled>Login</button>
    <p class="mt-5 mb-3 text-muted">&copy; 2020</p>
  </form>